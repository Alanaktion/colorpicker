  "bugs": {
    "url": "https://github.com/Alanaktion/colorpicker/issues"
  },
  "homepage": "https://github.com/Alanaktion/colorpicker#readme",
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "autoprefixer": "^9.6.5",
    "chroma-js": "^2.0.6",
    "color-blind": "^0.1.1",
    "gh-pages": "^2.1.1",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^6.1.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "tailwindcss": "^1.1.3"
  }
